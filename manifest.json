{
    "manifest_version": 3,
    "name": "Drake Bulk Event Register",
    "description": "Creates events in bulk on MyDrake web",
    "version": "1.0",
    "author": { "email": "eu@vfcardoso.dev" },
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "icons/icon-16.png",
            "32": "icons/icon-32.png",
            "48": "icons/icon-48.png",
            "128": "icons/icon-128.png"
        }
    },
    "permissions": ["tabs", "activeTab", "scripting"],
    "host_permissions": ["https://drake.bz/*", "https://*.drake.bz/*"]
}